  "scripts": {
    "start": "node app/server/app.js",
    "postinstall": "node node_modules/bower/bin/bower install",
    "debug": "node debug app/server/app.js",
    "test": "node node_modules/jasmine-node/lib/jasmine-node/cli.js tests/unit/server --forceexit ; (cd tests && karma start --single-run); npm run test_e2e",
    "test_unit": "node node_modules/jasmine-node/lib/jasmine-node/cli.js tests/unit/server --captureExceptions --forceexit",
    "test_unit_debug": "node debug node_modules/jasmine-node/lib/jasmine-node/cli.js tests/unit/server --captureExceptions --forceexit",
    "test_karma": "(cd tests && karma start)",
    "test_e2e": "node app/server/app.js -p 8000 > temp_test_log & ./node_modules/protractor-multi/bin/protractor-multi tests/protractor-multi_conf.js && rm temp_test_log; kill $!",
    "test_e2e_debug": "./node_modules/protractor-multi/bin/protractor-multi debug --debug=5859 tests/protractor-multi_conf.js",
    "es": "sudo service elasticsearch start"
  },
